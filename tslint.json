{
  "defaultSeverity": "error",
  "extends": [
    "tslint-react"
  ],
  "jsRules": {
  },
  "rules": {
    "member-access": false,
    "ordered-imports": false,
    "quotemark": false,
    "no-console": false,
    "semicolon": false,
    "jsx-no-lambda": false,
    "object-literal-sort-keys": false,
    "no-empty": [true, "allow-empty-functions"]
  },
  "rulesDirectory": [
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
